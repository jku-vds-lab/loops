    "@jupyterlab/hub-extension": "^4.0.2",
    "@jupyterlab/imageviewer": "^4.0.2",
    "@jupyterlab/imageviewer-extension": "^4.0.2",
    "@jupyterlab/inspector": "^4.0.2",
